@model WorkingExample.Models.Product
        <div class="span4">
            <div class="product-header-wrapper">
              <h1>@Model.Name</h1>
                <div class="price">
                    @Model.Price kr
                </div>
                @Html.ActionLink("Edit »", "Edit", "Product",new { productId = Model.Id }, new { Class = "btn"})
             
            </div>
            <div class="thumbnail thumbnail-big">
                <img src="@Html.Raw(Model.UrlToLargeImage)" />
            </div>
            <span class="label label-info"><a href="#">Tag #1</a></span> <span class="label label-info"><a href="#">Tag #2</a></span>
        </div>